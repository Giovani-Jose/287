<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>auberginnServlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Accueil</servlet-name>
        <servlet-class>auberginnServlet.Accueil</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Accueil</servlet-name>
        <url-pattern>/Accueil</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Inscription</servlet-name>
        <servlet-class>auberginnServlet.Inscription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Inscription</servlet-name>
        <url-pattern>/Inscription</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>auberginnServlet.Logout</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Gerer</servlet-name>
        <servlet-class>auberginnServlet.Gerer</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Gerer</servlet-name>
        <url-pattern>/GererMembre</url-pattern>
        <url-pattern>/GererChambre</url-pattern>
        <url-pattern>/GererCommodite</url-pattern>
        <url-pattern>/ActionChambre</url-pattern>
        <url-pattern>/ActionCommodite</url-pattern>

    </servlet-mapping>

    <servlet>
        <servlet-name>Gestion</servlet-name>
        <servlet-class>auberginnServlet.Gestion</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Gestion</servlet-name>
        <url-pattern>/GestionChambre</url-pattern>
        <url-pattern>/GestionCommodite</url-pattern>
        <url-pattern>/GestionReservation</url-pattern>
    </servlet-mapping>


</web-app>